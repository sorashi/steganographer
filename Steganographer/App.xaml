<Application x:Class="Steganographer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Steganographer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <Style x:Key="DesignerStyle" TargetType="Panel">
            <Setter Property="Background" Value="White"></Setter>
        </Style>
        <local:ToWritableBitmapConverter x:Key="ToWritableBitmapConverter"></local:ToWritableBitmapConverter>
        <local:IsNotNullToBooleanConverter x:Key="IsNotNullToBooleanConverter"></local:IsNotNullToBooleanConverter>
    </Application.Resources>
</Application>
